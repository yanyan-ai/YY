#include <stdio.h>
int main()
{
    int a, b, c, d;
    scanf("%d%d%d", &a, &b, &c);
    d = max(a, b,c);
    printf("他们中最大的数字:%d\n",d);
    return 0;
}
int max(int x, int y,int z)
{
    int k;
    if ((x > y) > z)
        k = x;
    else
        if (y > z)
            k = y;
        else
            k = z;
    return k;
}
